    "import time\n",
    "import random\n",
    "import math\n",
    "\n",
    "from lineplot import LinePlot\n",
    "\n",
    "plot = LinePlot(width=\"700px\", y_ticks=3, colors=['red', 'gold', 'purple'])\n",
    "\n",
    "for i in range(100):\n",
    "    time.sleep(0.1)\n",
    "    plot.add(loss=0.1+1/(0.5*i+1), random=random.random(), **{\"1/(i+1)\": 1/(i+1)})"
   ]
