    "import time\n",
    "import random\n",
    "\n",
    "from lineplot import LinePlot\n",
    "\n",
    "plot = LinePlot(width=\"700px\")\n",
    "\n",
    "for i in range(200):\n",
    "    time.sleep(0.025)\n",
    "    plot.add(x=1/(i+1), y=random.random())"
